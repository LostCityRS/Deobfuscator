[profile]
input_jar = "gamepack.jar"
output_jar = "gamepack" # in zwyz-full mode this appends .jar for you as part of a multi-step process
                        # otherwise make sure you use a unique name with a good file extension!
output_dir = "dump"

# run bytecode (objectweb.asm) transformers
class_deob = false
class_remap = false

# see org.runewiki.deob.bytecode.BytecodeDeobfuscator for a list of bytecode transformers
# pass them as strings minus the "Transformer" suffix

# pre-remap: it can be useful to have the original names available or shuffle things before remapping
pre_transformers = [
    # "OriginalName"
    # "SortClassesLegacy" # 2004
]

# post-remap
transformers = [
    # "Visibility",
    # "OpaquePredicate",
    # "ExceptionObfuscation",
    # "ExceptionTracing",
    # "Monitor",
    # "BitShift",
    # "FernflowerException",
    # "SortMethodsLineOrder"
]

# run decompiler (fernflower)
class_decompile = true

# run ast (javaparser) transformers
java_cleanup = false

# note: not every transformer is guaranteed to work. also, order matters. experiment a bit!
ast_transformers = [
    # "Unenclose",
    # "NegativeLiteral",
    # "Complement",
    # "Not",
    # "CharLiteral",
    # "IfElse",
    # "Ternary",
    # "BinaryExprOrder",
    # "AddSub",
    # "Identity",
    # "BitMask",
    # "HexLiteral",
    # "ValueOf",
    # "NewInstance",
    # "Increment",
    # "ForLoopCondition",
    # "RedundantCast",
    # "Enclose"
]

[profile.zwyz]
full = true # take over bytecode/remap transformations
math = true # apply runelite math deob
